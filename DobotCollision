collision = false;
for i = 1:6 %check every link mesh for collision
    for a = 1:size(Obstacles,1)
        %check link i for collision with mesh a
        if collision == true
            collision = true; %placeholder for logic
        end
    end
end

if collision == true
    %plot a course around obstacle
    
    
    %waypoint(i) = Plottraj()
